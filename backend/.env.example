# Database
DATABASE_URL="postgresql://user:user_password@localhost:5432/ccfvs_db?schema=public"

# Application
NODE_ENV=development
PORT=3001
API_VERSION=v1

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRATION=7d
JWT_ISSUER=your-issuer
JWT_AUDIENCE=your-audience

# CORS
CORS_ORIGIN=http://localhost:3000

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads

# WebSocket
WS_PORT=3001

# Rate Limiting
THROTTLE_TTL=60
THROTTLE_LIMIT=100
